<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="fooOne" class="com.jun.prospring5.ch03.beanAutowiring.FooImpl1" primary="true"/>
    <bean id="fooTwo" class="com.jun.prospring5.ch03.beanAutowiring.FooImpl2"/>
    <bean id="bar" class="com.jun.prospring5.ch03.beanAutowiring.Bar"></bean>
    <bean id="targetByType" autowire="byType" class="com.jun.prospring5.ch03.beanAutowiring.CTarget" lazy-init="true"></bean>

</beans>